    "definitions": {
        "basic:Typed": {
            "title": "Typed",
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "description": "type of this Typed"
                }
            },
            "required": [
                "type"
            ]
        },
        "basic:Entity": {
            "title": "Entity",
            "type": "object",
            "allOf": [
                {
                    "$ref": "#/definitions/basic:Typed"
                }
