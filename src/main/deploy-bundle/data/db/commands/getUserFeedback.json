{
  "aggregate": "user.feedbacks",
  "pipeline": [{
    "$match": {
      "dateTime": {dateTimeRange}
    }
  }, {
    "$project": {
      "_id": 1,
      "dateTime": 1,
      "userId": 1,
      "message": 1,
      "coordinates": 1,
      "url": {
        "$concat": [
          "{appDiagramUrl}",
          "$coordinates.diagramId",
          "/",
          "$coordinates.elementId"
        ]
      },
      "svg": {
        "$concat": [
          "{appDiagramSvg}",
          "$coordinates.diagramId",
          ".svg"
        ]
      }
    }
  }]
}