{
  "notation": "diagram:notation:FPMN",
  "name": "Formal Procedure Model and Notation",
  "description": "",
  "params": {
    "poolHeaderHeight": 40,
    "labelWidth": 220,
    "phaseHeight": 120,
    "phaseArrowWidth": 40
  },
  "stencils": [
    {
      "semanticType": "diagram:FPMN:semantic:Procedure",
      "elementType": "diagram:element:Shape",
      "type": "node",
      "safeArea": {
        "x": "element.bounds.x",
        "y": "element.bounds.y",
        "width": "element.bounds.width",
        "height": "params.poolHeaderHeight"
      },
      "view": {
        "silhouette": {
          "type": "rect",
          "attrs": {
            "x": "element.bounds.x",
            "y": "element.bounds.y",
            "width": "element.bounds.width",
            "height": "element.bounds.height"
          }
        },
        "paints": [
          {
            "type": "rect",
            "attrs": {
              "x": "element.bounds.x",
              "y": "element.bounds.y",
              "width": "element.bounds.width",
              "height": "element.bounds.height"
            },
            "styles": {
              "stroke-width": "2"
            }
          }
        ],
        "icons": [],
        "iconSets": [],
        "label": {
          "attrs": {
            "x": "element.bounds.x",
            "y": "element.bounds.y",
            "width": "element.bounds.width",
            "height": "params.poolHeaderHeight"
          },
          "align": {
            "h": "center",
            "v": "middle"
          },
          "text": {
            "dependsOn": [
              "name"
            ],
            "value": "semantic.name"
          },
          "styles": {
            "font-weight": "'bold'"
          }
        },
        "texts": []
      },
      "roles": [],
      "properties": {
      }
    },
    {
      "semanticType": "diagram:FPMN:semantic:Phase",
      "elementType": "diagram:element:Shape",
      "type": "node",
      "safeArea": {
        "x": "element.bounds.x + (semantic.prevPhaseId ? params.phaseArrowWidth : 0)",
        "y": "element.bounds.y",
        "width": "element.bounds.width - (semantic.prevPhaseId ? params.phaseArrowWidth : 0) - (semantic.nextPhaseId ? params.phaseArrowWidth : 0)",
        "height": "element.bounds.height"
      },
      "view": {
        "silhouette": {
          "type": "path",
          "attrs": {
            "path": [
              {
                "x": "element.bounds.x",
                "y": "element.bounds.y"
              },
              {
                "x": "element.bounds.x + element.bounds.width - (semantic.nextPhaseId ? params.phaseArrowWidth : 0)",
                "y": "element.bounds.y"
              },
              {
                "x": "element.bounds.x + element.bounds.width",
                "y": "element.bounds.y + element.bounds.height / 2"
              },
              {
                "x": "element.bounds.x + element.bounds.width - (semantic.nextPhaseId ? params.phaseArrowWidth : 0)",
                "y": "element.bounds.y + element.bounds.height"
              },
              {
                "x": "element.bounds.x",
                "y": "element.bounds.y + element.bounds.height"
              },
              {
                "x": "element.bounds.x + (semantic.prevPhaseId ? params.phaseArrowWidth : 0)",
                "y": "element.bounds.y + element.bounds.height / 2"
              }
            ]
          }
        },
        "paints": [
          {
            "type": "path",
            "attrs": {
              "path": [
                {
                  "x": "element.bounds.x",
                  "y": "element.bounds.y"
                },
                {
                  "x": "element.bounds.x + element.bounds.width - (semantic.nextPhaseId ? params.phaseArrowWidth : 0)",
                  "y": "element.bounds.y"
                },
                {
                  "x": "element.bounds.x + element.bounds.width",
                  "y": "element.bounds.y + element.bounds.height / 2"
                },
                {
                  "x": "element.bounds.x + element.bounds.width - (semantic.nextPhaseId ? params.phaseArrowWidth : 0)",
                  "y": "element.bounds.y + element.bounds.height"
                },
                {
                  "x": "element.bounds.x",
                  "y": "element.bounds.y + element.bounds.height"
                },
                {
                  "x": "element.bounds.x + (semantic.prevPhaseId ? params.phaseArrowWidth : 0)",
                  "y": "element.bounds.y + element.bounds.height / 2"
                }
              ]
            }
          }
        ],
        "icons": [],
        "iconSets": [
          {
            "basePoint": {
              "x": "element.bounds.x + (semantic.prevPhaseId ? params.phaseArrowWidth : 0) + 2",
              "y": "element.bounds.y + 2"
            },
            "settings": {
              "width": 24,
              "height": 24,
              "h": "left",
              "v": "top",
              "direction": "right"
            },
            "refs": [
              {
                "id": "graphics-e-service",
                "display": {
                  "dependsOn": [
                    "eServiceId"
                  ],
                  "value": "semantic.eServiceId != null"
                },
                "styles": {}
              }
            ]
          }
        ],
        "label": {
          "attrs": {
            "x": "element.bounds.x + (semantic.prevPhaseId ? params.phaseArrowWidth : 0)",
            "y": "element.bounds.y",
            "width": "element.bounds.width - (semantic.prevPhaseId ? params.phaseArrowWidth : 0) - (semantic.nextPhaseId ? params.phaseArrowWidth : 0)",
            "height": "element.bounds.height"
          },
          "align": {
            "h": "center",
            "v": "middle"
          },
          "text": {
            "dependsOn": [
              "name"
            ],
            "value": "semantic.name"
          }
        },
        "texts": []
      },
      "roles": [],
      "properties": {
      }
    }
  ],
  "rules": {
  }
}
